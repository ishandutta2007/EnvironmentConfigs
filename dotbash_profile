export PATH="/Users/ishandutta2007/Library/Android/sdk/tools/bin:$PATH"
export PATH="/Users/ishandutta2007/Library/Android/sdk/platform-tools:$PATH"

export ANDROID_HOME=$HOME/Library/Android/sdk
export ANDROID_SDK=$HOME/Library/Android/sdk
export PATH=$ANDROID_SDK/emulator:$ANDROID_SDK/tools:$PATH

export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init -)"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

export PS1="\[\033[36m\]\u\[\033[m\]@\[\033[32m\]\h:\[\033[33;1m\]\w\[\033[m\]\$ "
export CLICOLOR=1
export LSCOLORS=ExFxBxDxCxegedabagacad
alias ls='ls -GFh'
alias cat='ccat'
alias chrome='open /Applications/Google\ Chrome.app'
alias code='open /Applications/Visual\ Studio\ Code.app'
alias db='open /Applications/DB\ Browser\ for\ SQLite.app'
alias charm='open /Applications/PyCharm\ CE.app'
alias sublime='open /Applications/Sublime\ Text.app'
alias studio='open /Applications/Android\ Studio.app'


